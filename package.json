{
  "name": "webpack4-demo",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:lihaichen/webpack4Demo.git",
  "author": "lihaichen <li8303@163.com>",
  "license": "MIT",
  "scripts": {
    "dev": "better-npm-run gen-dev-config &&  better-npm-run dev"
  },
  "betterScripts": {
    "gen-dev-config": {
      "command": "node ./cmd/generate_config.js",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "dev": {
      "command": "node_modules/.bin/webpack-dev-server --mode development --config ./webpack/dev.config.js",
      "env": {
        "NODE_ENV": "dev"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "better-npm-run": "^0.1.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "less-loader": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "dva": "^2.4.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  }
}
